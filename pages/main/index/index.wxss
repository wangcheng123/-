/* mains.wxss */

.container_mains {
  width: 100%;
  height: 100%;
}

/* banner */

.swiperbox {
  height: 330rpx;
  text-align: center;
  color: #fff;
  padding-top: 84rpx;
}

.swiperbox image {
  width: 100%;
  height: 100%;
}

/* 公告 */

.notice {
  display: flex;
  height: 84rpx;
  background: rgba(255, 248, 209, 1);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.notice .noticetxt {
  width: 68rpx;
  height: 36rpx;
  border-radius: 8rpx;
  border: 2rpx solid rgba(255, 56, 62, 1);
  font-size: 24rpx;
  line-height: 36rpx;
  color: rgba(255, 56, 62, 1);
  text-align: center;
  margin: 15rpx 0rpx 15rpx 30rpx;
}

.sildernotice {
  width: 68%;
  height: 64rpx;
  line-height: 64rpx;
  white-space: nowrap;
  font-size: 0;
  margin-left: 20rpx;
}

.sildernotice text {
  display: inline-block;
  font-size: 28rpx;
  line-height: 64rpx;
  color: rgba(221, 151, 98, 1);
}

/* 公告更多 */

.notice .notice_more {
  width: 100rpx;
  height: 64rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  line-height: 64rpx;
  text-align: left;
  padding-left: 30rpx;
  z-index: 10;
}

.notice_more::before {
  content: "";
  width: 2rpx;
  height: 24rpx;
  background-color: rgba(153, 153, 153, 1);
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}

.moretxt {
  width: 100%;
}

.decollator {
  padding-right: 35rpx;
  color: #e5e5e5;
}

/* 快速预约 预约单 我的业绩 客服 */

.botitem {
  margin: 50rpx 30rpx;
  display: flex;
  justify-content: space-between;
  text-align: center;
}

.botitem .itemlist, .botitem .itemlist image {
  width: 210rpx;
  height: 130rpx;
}

.botitem.order_box {
  justify-content: flex-start;
  flex-flow: wrap;
}

.botitem.order_box .itemlist {
  width: 22%;
  height: auto;
  margin: 0 1.5% 58rpx;
}

.botitem .itemlist view:first-child {
  width: 60rpx;
  height: 60rpx;
}

.botitem .itemlist view:last-child {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

button.itemlist.ser {
  margin: 0;
  padding: 0;
  background: #fff;
  border: 0;
}

button.itemlist.ser view:last-child {
  height: 38rpx;
  line-height: 38rpx;
}

.botitem.order_box .itemlist .icon {
  margin: 0 auto 16rpx;
}

.botitem .itemlist:nth-child(1) .icon {
  background: url(https://img.ameimeika.com/h5_images/businessAssistant/icon_home_myreward.png) no-repeat;
  background-size: 100% 100%;
}

.botitem .itemlist:nth-child(2) .icon {
  background: url(https://img.ameimeika.com/h5_images/businessAssistant/icon_home_myachievement.png) no-repeat;
  background-size: 100% 100%;
}

.botitem .itemlist:nth-child(3) .icon {
  background: url(https://img.ameimeika.com/h5_images/businessAssistant/icon_home_myteam.png) no-repeat;
  background-size: 100% 100%;
}

.botitem .itemlist:nth-child(4) .icon {
  background: url(https://img.ameimeika.com/h5_images/businessAssistant/icon_home_myorder.png) no-repeat;
  background-size: 100% 100%;
}

/* 客服菜单 */

.botitem .itemlist .kefu {
  margin: 0 auto 16rpx;
  background: url(https://img.ameimeika.com/h5_images/businessAssistant/icon_home_service.png) no-repeat;
  background-size: 100% 100%;
  width: 60rpx;
  height: 60rpx;
}

/* 推荐业绩和分销业绩tab */

.pertab {
  width: 100%;
  height: 88rpx;
  background: rgba(245, 245, 245, 1);
  line-height: 75rpx;
  text-align: center;
}

.pertab text {
  font-size: 28rpx;
  margin: 0 55rpx;
  color: rgba(51, 51, 51, 1);
}

.pertab text.changetab {
  color: rgba(255, 56, 62, 1);
  border-bottom: 4rpx solid rgba(255, 56, 62, 1);
}

/* 业绩面板 */

.center_data {
  height: 248rpx;
  margin: 30rpx 30rpx 5rpx;
  border-radius: 6px;
  font-size: 0;
}

.center_data .center_top {
  display: flex;
  justify-content: space-between;
}

.center_data .center_top .monthper {
  font-size: 24rpx;
  color: rgba(51, 51, 51, 1);
}

.center_data .performance {
  margin: 0rpx 0 14rpx;
  text-align: center;
}

.center_data .performance text:first-child {
  font-size: 28rpx;
  color: rgba(255, 56, 62, 1);
  margin-right: 20rpx;
}

.center_data .performance text:last-child {
  font-size: 55rpx;
  color: rgba(255, 56, 62, 1);
}

.center_data .ranklist {
  display: flex;
  text-align: center;
  padding: 0 12%;
  margin-bottom: 14rpx;
}

.center_data .rank {
  width: 50%;
  text-align: center;
  font-size: 24rpx;
  color: rgba(102, 102, 102, 1);
}

.center_data .rank text {
  font-size: 34rpx;
  margin: 0 10rpx;
  font-weight: 600;
}

.center_data .center_bottxt {
  text-align: center;
  height: 40rpx;
}

.center_data .center_bottxt image {
  display: inline-block;
  width: 27rpx;
  height: 27rpx;
  margin: -3rpx 10rpx;
}

.center_data .center_bottxt text {
  display: inline-block;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.line {
  margin-top: 16rpx;
  border: 1rpx solid rgba(245, 245, 245, 1);
}

/* 最高业绩 最高奖励 */

.highestdata {
  margin: 10rpx 90rpx 30rpx;
  display: flex;
  justify-content: space-between;
  height: 128rpx;
  text-align: center;
}

.highest .htxt {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.highest .hmoney {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
  margin: 15rpx 0;
}

.highest .hfrom {
  font-size: 12px;
  color: rgba(136, 136, 136, 1);
  margin-top: 6rpx;
}

.highest .hfrom text {
  margin: 0 10rpx;
}

/* swpier指示点样式 */

.swiperbox .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 2rpx;
}

.swiperbox .wx-swiper-dot {
  width: 16rpx;
  display: inline-flex;
  height: 6rpx;
  margin-left: 8rpx;
  justify-content: space-between;
}

.swiperbox .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 8rpx;
}

.swiperbox .wx-swiper-dot-active::before {
  background: #ff383e;
}

/* 阴影层  */

.shadow {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 100;
}

/*新人专享窗*/

.shadow .active {
  position: relative;
  width: 480rpx;
  left: 50%;
  top: 45%;
  margin-top: -275rpx;
  margin-left: -240rpx;
}

.shadow .active image, .reward image {
  width: 100%;
}

.shadow .active view {
  position: absolute;
  z-index: 110;
  top: 0;
  right: 0;
  color: #fff;
  font-size: 28rpx;
  width: 88rpx;
  height: 86rpx;
  background: #313131;
  border-bottom-left-radius: 100%;
}

.shadow .active view text {
  line-height: 60rpx;
  text-align: right;
  position: absolute;
  top: 5rpx;
  right: 10rpx;
}

/*推荐金额窗*/

.shadow .reward {
  position: relative;
  width: 550rpx;
  left: 50%;
  top: 45%;
  margin-top: -245rpx;
  margin-left: -275rpx;
}

.shadow .reward .money {
  color: #dd5e6a;
  font-size: 67rpx;
  text-align: center;
  line-height: 79rpx;
  width: 240rpx;
  position: absolute;
  top: 235rpx;
  left: 150rpx;
}

.shadow .reward .money text {
  font-size: 40rpx;
}

.shadow .reward .ok {
  position: absolute;
  top: 375rpx;
  left: 0;
  width: 550rpx;
  height: 83rpx;
  color: #fff;
  font-size: 43rpx;
  text-align: center;
  line-height: 83rpx;
}

/* 营业额 */

.turnover {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  margin-top: 54rpx;
}

.turnover .turnover_line {
  width: 2rpx;
  height: 100rpx;
  background: rgba(216, 216, 216, 1);
}

.turnover_item .item_title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 12rpx;
}

.turnover_item .item_title image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}

.turnover_item .item_num {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFangSC;
  font-weight: 500;
  color: rgba(201, 38, 23, 1);
  text-align: center;
}

.bottom_logo {
  margin: 0 118rpx 118rpx;
}

.bottom_logo image {
  width: 100%;
}

/* 核销结果弹框 */

.cancel_result {
  width: 100%;
  height: 100%;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.cancel_result image {
  width: 200rpx;
  height: 200rpx;
}

.cancel_result .title {
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin: 30rpx 0 80rpx;
}

.cancel_result .money {
  font-size: 60rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(255, 69, 69, 1);
  margin-bottom: 20rpx;
}

.cancel_result text {
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.cancel_result .cancel_btn {
  position: absolute;
  bottom: 140rpx;
  left: 50%;
  transform: translateX(-50%);
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.cancel_result .cancel_btn .result_btn{
width:460rpx;
height:100rpx;
background:rgba(255,56,62,1);
border-radius:50rpx;
font-size:32rpx;
font-family:PingFangSC;
font-weight:500;
color:rgba(255,255,255,1);
line-height: 100rpx;
text-align: center;
}
